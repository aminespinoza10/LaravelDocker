# Example environment variables for local development and deployment
# Copy this file to .env.local and fill in your actual values
# DO NOT commit .env.local to git

# Laravel Application
APP_KEY=  # Generated via: docker run --rm laravel-app:local php artisan key:generate --show

# Azure MySQL Database
DB_HOST=soy-leon-developer.mysql.database.azure.com
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=leonadmin
DB_PASSWORD=  # Your Azure MySQL password

# Azure Container Registry (for deployment)
ACR_NAME=soyleonsandboxacr
ACR_USERNAME=  # Your ACR username
ACR_PASSWORD=  # Your ACR password or access token
